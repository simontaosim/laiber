<!doctype html>
<html class="no-js">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="description" content="">
  <meta name="keywords" content="">
  <meta name="viewport"
        content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
  <title>Ulaiber登陆页面</title>

  <!-- Set render engine for 360 browser -->
  <meta name="renderer" content="webkit">

  <!-- No Baidu Siteapp-->
  <meta http-equiv="Cache-Control" content="no-siteapp"/>

  <link rel="icon" type="image/png" href="assets/i/favicon.png">

  <!-- Add to homescreen for Chrome on Android -->
  <meta name="mobile-web-app-capable" content="yes">
  <%= favicon_link_tag 'app-icon72x72@2x.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '144x144' %>

  <!-- Add to homescreen for Safari on iOS -->
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black">
  <meta name="apple-mobile-web-app-title" content="Amaze UI"/>
  <%= favicon_link_tag 'app-icon72x72@2x.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '144x144' %>
  <%= stylesheet_link_tag    'amazeui.min.css', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'app.css', media: 'all', 'data-turbolinks-track' => true %>
  
  <%= csrf_meta_tags %>
</head>
<body>
<div class="nav">
     <ul class="am-nav am-nav-tabs">
      <li class="am-active ">
      <li><a href="#">登陆</a></li>
    </ul>
</div>

<!--[if (gte IE 9)|!(IE)]><    !-->
<%= javascript_include_tag 'jquery.min.js', 'data-turbolinks-track' => true %>
<%= yield %>
<%= javascript_include_tag 'amazeui.min.js', 'data-turbolinks-track' => true %>
<!--<![endif]-->

</body>
</html>
